{
    "name": "@purinton/errors",
    "type": "module",
    "version": "1.0.1",
    "description": "A generic handler for catching uncaught errors, warnings, and rejections.",
    "main": "index.cjs",
    "module": "index.mjs",
    "types": "index.d.ts",
    "files": [
        "index.mjs",
        "index.cjs",
        "index.d.ts",
        "README.md",
        "LICENSE"
    ],
    "exports": {
        ".": {
            "import": "./index.mjs",
            "require": "./index.cjs"
        }
    },
    "scripts": {
        "test": "npx --node-options=\"--experimental-vm-modules --no-warnings\" jest --detectOpenHandles --silent"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/purinton/errors.git"
    },
    "keywords": [
        "nodejs",
        "module",
        "template",
        "boilerplate",
        "javascript",
        "typescript",
        "esm",
        "commonjs",
        "cjs",
        "package",
        "library",
        "npm",
        "testing",
        "jest",
        "ci",
        "continuous-integration",
        "node"
    ],
    "author": "Russell Purinton <russell@purinton.us>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/purinton/errors/issues"
    },
    "homepage": "https://github.com/purinton/errors#readme",
    "devDependencies": {
        "jest": "^29.7.0"
    },
    "jest": {
        "testEnvironment": "node",
        "testMatch": [
            "**/?(*.)+(spec|test).[jt]s?(x)",
            "**/?(*.)+(spec|test).mjs",
            "**/?(*.)+(spec|test).cjs"
        ]
    },
    "dependencies": {
        "@purinton/log": "^1.0.2"
    }
}